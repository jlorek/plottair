<div class="jumbotron">
  <h2><%= gettext "Welcome to %{name}!", name: "Plottair" %></h2>
  <p class="lead">Upload your stuff here!</p>

  <%= form_for @conn, page_path(@conn, :upload), [as: "upload", multipart: :true], fn f -> %>  
    <%# <%= file_input f, :file, accept: "*/*" %>
    <%= text_input f, :command %>
    <button class="btn btn-primary" type="submit">Upload</button>
  <% end %>

</div>